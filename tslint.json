{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-standard",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": true,
        "object-literal-sort-keys": false,
        "no-console": true,
        "interface-name": true,
        "class-name": true,
        "arrow-return-shorthand": true,
        "no-floating-promises": false
    },
    "rulesDirectory": []
}